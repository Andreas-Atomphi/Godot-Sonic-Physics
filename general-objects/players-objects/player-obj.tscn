func animation_step(host: PlayerPhysics, animator: CharacterAnimator, delta, state = null, args=[]):
	var anim_name = args[0]
	var anim_speed = args[1]
	#print(state)
	if !state:
		return
	if state.has_jumped or state.has_rolled:
		if dropCharging:
			anim_name = 'DropCharge';
			anim_speed = 4.5;
		
	return {
		\"anim_name\": anim_name,
		\"anim_speed\": anim_speed
	}

func _dropTimeOut(host, delta) -> void:
	dropCharging = true;
	host.audio_player.play('drop_dash_charge');
	dropTimer.stop();

func cancel_drop() -> void:
	dropPress = false;
	if (dropTimer != null):
		dropTimer.stop();
		if has_node(dropTimer.name):
			dropTimer.queue_free();
			dropTimer = null;
		dropCharging = false;
